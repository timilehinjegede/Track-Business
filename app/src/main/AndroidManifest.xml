<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.timilehinjegede.track_business">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.VoucherActivity" />
        <activity android:name=".Activities.SaleHistoryActivity" />
        <activity android:name=".Activities.PurchaseHistoryActivity"/>
        <activity android:name=".Activities.SalesActivity" />
        <activity android:name=".Activities.PurchaseActivity" />
        <activity android:name=".Activities.InventoryActivity" />
        <activity android:name=".Activities.KnowYouActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.MainActivity" />
    </application>

</manifest>